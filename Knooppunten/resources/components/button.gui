<defs>
  <symbol id="button" height="50">
    <rect fill="inherit" x="4" y="0" width="100%-8" height="100%" rx="10" ry="10" />
    <rect fill="black" x="6" y="2" width="100%-12" height="100%-4" rx="10" ry="10"/>
    <text id="text" fill="white" x="50%" y="70%" font-size="32" font-family="System-Regular" text-anchor="middle" text-buffer="continue" />
    <rect id="handle" fill="black" x="0" y="0" width="100%" height="100%" opacity="0" pointer-events="visible"/>
  </symbol>
  <symbol id="buttoninfo" height="50">
      <circle cx="80" cy="250" r="45" fill="gray" />
      <circle cx="80" cy="250" r="42"   fill="#000000" />
      <textarea id="speed" fill="white" x="55" y="230" width="45" height="110" font-size="20" font-family="System-Regular" text-anchor="middle" text-buffer="15km/h" />
      <rect id="handletwee" fill="black" x="0" y="0" width="100%" height="100%" opacity="0" pointer-events="visible"/>
  </symbol>
</defs>
